<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="760.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="views.CustomerMainController">
   <children>
      <Label layoutX="466.0" layoutY="26.0" text="Customers">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="customerTable" layoutX="62.0" layoutY="68.0" prefHeight="600.0" prefWidth="900.0">
        <columns>
          <TableColumn fx:id="customerId" prefWidth="91.0" text="Customer ID" />
          <TableColumn fx:id="customerName" prefWidth="154.0" text="Name" />
            <TableColumn fx:id="customerStatus" prefWidth="55.0" text="Active" />
            <TableColumn fx:id="customerAddress" prefWidth="102.0" text="Address" />
            <TableColumn fx:id="customerAddress2" prefWidth="115.0" text="Address 2" />
            <TableColumn fx:id="customerPostal" prefWidth="97.0" text="Postal Code" />
            <TableColumn fx:id="customerPhone" prefWidth="85.0" text="Phone" />
            <TableColumn fx:id="customerCity" prefWidth="93.0" text="City" />
            <TableColumn fx:id="customerCountry" prefWidth="107.0" text="Country" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="48.0" layoutY="697.0" mnemonicParsing="false" onAction="#backButtonPushed" text="Back" />
      <Button fx:id="addCustButton" layoutX="306.0" layoutY="697.0" mnemonicParsing="false" onAction="#addCustButtonPushed" text="Add Customer" />
      <Button fx:id="modCustButton" layoutX="450.0" layoutY="697.0" mnemonicParsing="false" onAction="#modCustButtonPushed" text="Modify Customer" />
      <Button fx:id="delCustButton" layoutX="609.0" layoutY="697.0" mnemonicParsing="false" onAction="#delCustButtonPushed" text="Delete Customer" />
   </children>
</AnchorPane>
